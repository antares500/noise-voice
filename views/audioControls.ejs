<div class="row">	
	<section class="col-md-2 panel panel-default mic">
		<h3>Mic</h3>
		<p class="mic-status label label-default">off</p>
	</section><!--Mic-->
	
	<section class="col-md-3 panel panel-default">
		<h3>Delay</h3>
		<div class="checkbox">
			<label><input type="checkbox" value="" class="delay-switch" checked>on/off</label>
		</div>
		<div class="range">
			<label>delay time (0-5 sec): <input type="range" min="0" max="5" value="0" step="0.5"  class="delay-value"></label>
		</div>
	</section><!--Delay-->
	
	<section class="col-md-3 panel panel-default">
		<h3>Chorus (tuna)</h3>
		<div class="checkbox">
			<label><input type="checkbox" value="" class="tunachorus-switch" checked>on/off</label>
		</div>
		<div class="range">
			<label>rate: <input type="range" name="rate" min="0.01" max="8" value="1.5"  step="0.01" class="tunachorus-rate"></label>
		</div>
		<div class="range">
			<label>feedback: <input type="range"  name="feedback" min="0" max="1" value="0.2"  step="0.1"  class="tunachorus-feedback"></label>
		</div>
		<div class="range">
			<label>delay: <input type="range"  name="delay" min="0" max="1" value="0.004"  step="0.1"  class="tunachorus-delay"></label>
		</div>
	</section><!--TunaChorus-->
	
	<section class="col-md-3 panel panel-default">
		<h3>Gain</h3>
		<div class="checkbox">
			<label><input type="checkbox" value="" class="gain-switch" checked>on/off</label>
		</div>
		<div class="range">
			<label>volume: <input type="range" min="0" max="1" value="0.5" step="0.1" class="gain-value"></label>
		</div>
	</section><!--Gain-->
	
</div><!--row-->