<div class="row-fluid">	
	<section class="col-sm-3 panel panel-default mic">
		
		<h3>Microphone</h3>
		<span class="mic-status label label-default">off</span>
		<div class="range">
			<label>volume: <input type="range" min="0" max="1" value="0.5" step="0.1" class="gain-value"></label>
		</div>
	</section><!--Mic and gain-->
	
	<section class="col-sm-3 panel panel-default">
		<h3>Delay</h3>
		<div class="checkbox">
			<label><input type="checkbox" value="" class="delay-switch" checked>on/off</label>
		</div>
		<div class="range">
			<label>delay time (0-5 sec): <input type="range" min="0" max="5" value="0" step="0.5"  class="delay-value"></label>
		</div>
	</section><!--Delay-->
	
	<section class="col-sm-3 panel panel-default">
		<h3>Chorus (tuna)</h3>
		<div class="checkbox">
			<label><input type="checkbox" value="" class="tunachorus-switch" checked>on/off</label>
		</div>
		<div class="range">
			<label>Pitch: <input type="range"  name="delay" min="0" max="1" value="0.004"  step="0.1"  class="tunachorus-delay"></label>
		</div>
	</section><!--TunaChorus-->
	
</div><!--row-->